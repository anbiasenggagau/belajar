<div class="container">
    <div class="row">
        <div class="col-md-8">
            <h3>Daftar Contact</h3>
            <table class="table">
                <thead>
                    <tr>
                        <th scope="col">No.</th>
                        <th scope="col">Nama</th>
                        <th scope="col">Nomor HP</th>
                        <th scope="col">Info</th>
                    </tr>
                </thead>
                <tbody>
                    <% contacts.forEach((element,index)=> { %>


                        <tr>
                            <th scope="row">
                                <%= index+1 %>
                            </th>
                            <td>
                                <%= element.nama %>
                            </td>
                            <td>
                                <%= element.nohp %>
                            </td>
                            <td><a href="/about/?nama=<%= element.nama %>"
                                    class="btn btn-success badge badge-pill">Detail</a>
                            </td>
                        </tr>
                        <% }) %>
                </tbody>
            </table>
        </div>
    </div>
</div>